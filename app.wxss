    url('https://saler.ishangbin.com/font/webfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('https://saler.ishangbin.com/font/webfont.svg#webfont') format('svg'); /* iOS 4.1- */
}
@font-face {
    font-family: 'webfont2';
    font-display: swap;
    src: url('https://saler.ishangbin.com/font/webfont2.eot'); /* IE9 */
