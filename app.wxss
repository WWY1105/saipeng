}
.ellipsis{
   overflow: hidden;
text-overflow: ellipsis;
white-space: nowrap;
}
.flexCenterBox{
   display: flex;
   align-items: center;
   justify-content: center;
}
rich-text{
   color: #000;
   font-size: 24rpx;
   line-height: 47rpx;
}
page {
   background-color: #f8f8f8;
   font-family: PingFangSC;
   font-weight: normal;
   font-style: normal;
   font-stretch: normal;
   line-height: normal;
   letter-spacing: normal;
   color: #fff;
}

page::after {
   content: '';
   position: fixed;
   left: 0;
   bottom: 0;
   width: 100%;
   height: 2rpx;
   background-color: #f3f3f3;
   z-index: 9999;
}
view{
   line-height: 1;
}
button::after {
   display: none;
}

.container {
   height: 100%;
   display: flex;
   flex-direction: column;
   align-items: center;
   justify-content: space-between;
   padding: 100.4rpx 0;
   box-sizing: border-box;
}

.navigationBar {
   height: 92rpx;
   width: 750rpx;
}

.left {
   float: left;
}

.right {
   float: right;
}

/* 上拉加载动画 */

.weui-loadmore {
   width: 100%;
   height: 100rpx;
   font-size: 30rpx;
   text-align: center;
   margin-top: 30rpx;
   color: #333;
}

.weui-loading {
   width: 50rpx;
   height: 50rpx;
   display: inline-block;
   vertical-align: middle;
   -webkit-animation: weuiLoading 1s steps(12, end) infinite;
   animation: weuiLoading 1s steps(12, end) infinite;
   background: transparent url(data:image/svg+xml;base64,) no-repeat;
   background-size: 100%;
}

.weui-loadmore-tips {
   display: inline-block;
   vertical-align: middle;
   color: #333;
   text-align: center;
   padding: 30rpx 0;
   width: 100%;
}

/* 定义全单动画 */

@keyframes big_small {
   0% {
      transform: scale(1); /*开始为原始大小*/
   }

   25% {
      transform: scale(1.3); /*放大1.1倍*/
   }

   50% {
      transform: scale(1);
   }

   75% {
      transform: scale(1.3);
   }

   100% {
      transform: scale(1); /*开始为原始大小*/
   }
}

.quandan_img {
   animation: big_small 1s infinite;
}

.quandan_img.static {
   animation: none;
}

