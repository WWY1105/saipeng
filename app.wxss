    url('https://saler.ishangbin.com/font/webfont2.woff') format('woff'), /* chrome、firefox */
    url('https://saler.ishangbin.com/font/webfont2.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('https://saler.ishangbin.com/font/webfont2.svg#webfont') format('svg'); /* iOS 4.1- */
}
